#include <iostream>
using namespace std;

int main() {
    
    int n,i,t,m;
    cin>>n;
    int arr[n];
    m = n-1;
    
    for(i=0;i<n;i++)
    cin>>arr[i];
    
    for(i=0;i<=m;i++){
        
        t=arr[i];
        arr[i]=arr[m];
        arr[m]=t;
        m--;
    }
    
    for(i=0;i<n;i++)
    cout<<arr[i]<<" ";
    
    
	// your code goes here
	return 0;
}
